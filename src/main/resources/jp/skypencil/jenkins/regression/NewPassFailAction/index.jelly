<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:l="/lib/layout">
  <l:layout title="Newly Pass/Fail Tests">
    <st:include it="${it.project}" page="sidepanel.jelly" />
    <st:bind var="remoteAction" value="${it}"/> 
    <l:main-panel>
    <div>
		<tbody>
		<table border="1">
			<tr>
       			<th>Newly Passed Tests</th>
       			<th>Newly Failed Tests</th>
    		</tr>
    		    		
		</table>
		</tbody>
		</div>
    </l:main-panel>
  </l:layout>
</j:jelly>